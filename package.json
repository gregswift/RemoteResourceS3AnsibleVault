{
  "name": "@razee/remoteresources3ansiblevault",
  "version": "0.0.0-dev",
  "description": "Razeedeploy: component to download and manage files from s3 object storage, and decrypt them using Ansible Vault",
  "main": "main.js",
  "keywords": [
    "kubernetes",
    "razee",
    "razeedash",
    "download",
    "cos",
    "s3",
    "object",
    "storage",
    "decrypt",
    "ansible",
    "ansible-vault"
  ],
  "scripts": {
    "start": "node src/index.js",
    "test": "nyc --reporter=html --all --reporter=text mocha ",
    "test:debug": "mocha --inspect-brk",
    "check-coverage": "nyc check-coverage --statements 0 --branches 0 --functions 0 --lines 0",
    "lint": "node_modules/.bin/npm-run-all eslint yamllint markdownlint",
    "eslint": "node_modules/.bin/eslint src/",
    "yamllint": "node_modules/.bin/yamllint .travis.yml kubernetes/RemoteResourceS3AnsibleVault/resource.yaml",
    "markdownlint": "node_modules/.bin/markdownlint README.md docs/",
    "dockerlint": "node_modules/.bin/dockerlint Dockerfile"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:logdna/RemoteResourceS3AnsibleVault.git"
  },
  "contributors": [
    {
      "name": "Greg Swift"
    }
  ],
  "release-it": {
    "git": {
      "tag": false
    },
    "publishConfig": {
      "access": "public"
    }
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@razee/kubernetes-util": "^0.1.6",
    "@razee/razeedeploy-core": "^0.9.1",
    "@razee/remoteresources3": "^0.6.4",
    "ansible-vault": "^1.0.0",
    "bunyan": "^1.8.14",
    "object-path": "^0.11.5"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "dockerlint": "^0.3.9",
    "eslint": "^7.11.0",
    "markdownlint-cli": "^0.23.2",
    "mocha": "^8.1.3",
    "nock": "^13.0.4",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "release-it": "^14.2.2",
    "rewire": "^5.0.0",
    "sinon": "^9.2.0",
    "yaml-lint": "^1.2.4"
  }
}
